{
  "name": "@teleporthq/teleport-renderer-react",
  "version": "0.0.16",
  "description": "React renderer for teleportHQ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "link-dev": "npm link ../teleport-lib-js",
    "clean": "rm -rf dist",
    "lint": "tslint -p ./",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage --no-cache",
    "upload:coverage": "codecov",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:teleporthq/teleport-renderer-react.git"
  },
  "author": "teleportHQ",
  "license": "MIT",
  "homepage": "https://gitlab.com/teleporthq/teleport-renderer-react#README",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.9",
    "codecov": "^3.0.4",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@teleporthq/teleport-lib-js": "^0.0.1",
    "react": "^16.4.2",
    "react-jss": "^8.6.1"
  }
}
